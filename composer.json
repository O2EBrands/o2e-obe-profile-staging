{
  "name": "o2ebrands/o2e_obe_profile_staging",
  "description": "This install profile is used for the D9 OBEs.",
  "type": "drupal-profile",
  "autoload": {
      "psr-4": {
          "O2ebrands\\O2eObeProfileStaging\\": "src/"
      }
  },
  "authors": [
      {
          "name": "Daryl Bautista",
          "email": "daryl.bautista@o2ebrands.com"
      }
  ],
  "repositories": [
    {
      "type": "package",
      "package": {
          "name": "commerceguys/addressing",
          "version": "v1.3.0",
          "type": "drupal-library",
          "dist": {
              "url": "https://github.com/commerceguys/addressing/archive/refs/heads/master.zip",
              "type": "zip"
          },
          "source": {
              "url": "https://github.com/commerceguys/addressing.git",
              "type": "git",
              "reference": "566febd56ca71e31dd383b014c4e1bec680507bf"
          }
      }
    }
  ],
  "require": {
    "drupal/admin_toolbar": "^3.1",
    "drupal/allowed_formats": "^1.4",
    "drupal/big_pipe_sessionless": "^2.0",
    "drupal/bootstrap_barrio": "^5.5",
    "drupal/bootstrap_sass": "^5.0",
    "drupal/config_filter": "^2.0",
    "drupal/config_ignore": "^2.3",
    "drupal/config_readonly": "^1.0@beta",
    "drupal/config_split": "^2.0",
    "drupal/core-composer-scaffold": "*",
    "drupal/core-recommended": "^9.3.9",
    "drupal/layout_builder_admin_theme": "^1.1",
    "drupal/layout_builder_modal": "^1.1",
    "drupal/layout_builder_operation_link": "^2.0",
    "drupal/layout_builder_restrictions": "^2.12",
    "drupal/metatag": "^1.19",
    "drupal/pathauto": "^1.8",
    "drupal/redirect": "^1.7",
    "drupal/robotstxt": "^1.4",
    "drupal/seckit": "^2.0",
    "drupal/security_review": "^1.0@alpha",
    "drupal/taxonomy_access_fix": "^3.3",
    "drupal/token": "^1.10",
    "drupal/webform": "^6.1",
    "drupal/xmlsitemap": "^1.2",
    "drush/drush": "^11 || ^12",
    "drupal/reroute_email": "^2.1",
    "drupal/console": "^1.9",
    "drupal/key": "^1.15",
    "kamermans/guzzle-oauth2-subscriber": "~1.0",
    "commerceguys/addressing": "^1.3"
  },
  "require-dev": {
    "drupal/core-dev": "~9.3.3",
    "drupal/devel": "^4.0",
    "symfony/phpunit-bridge": "^6.0",
    "weitzman/drupal-test-traits": "^1.5"
  },
  "extra": {
    "composer-exit-on-patch-failure": true,
    "drupal-scaffold": {
      "file-mapping": {
        "[project-root]/.editorconfig": false,
        "[project-root]/.gitattributes": false,
        "[web-root]/.csslintrc": false,
        "[web-root]/INSTALL.txt": false,
        "[web-root]/example.gitignore": false,
        "[web-root]/modules/README.txt": false,
        "[web-root]/profiles/README.txt": false,
        "[web-root]/robots.txt": false,
        "[web-root]/sites/README.txt": false,
        "[web-root]/themes/README.txt": false,
        "[web-root]/web.config": false
      },
      "locations": {
        "web-root": "web/"
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    }
  }
}
