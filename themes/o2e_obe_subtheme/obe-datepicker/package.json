{
  "name": "obe-datepicker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.39",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp build/static/js/main*.js ../js/datepicker-build.min.js && cp build/static/js/*.chunk.js ../js/react-chunk.min.js && cp build/static/css/main*.css ../css/react-chunk.css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "preinstall": "npx npm-force-resolutions"
  },
  "resolutions": {
    "nth-check": ">=2.0.1",
    "loader-utils": ">=3.2.1",
    "minimatch": ">=3.0.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "globals": {
      "Drupal": true,
      "jQuery": true,
      "once": true,
      "drupalSettings": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
